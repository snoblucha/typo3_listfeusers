<T3DataStructure>
    <sheets>
        <default>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:listfeusers/pi3/locallang.xml:mapTitle</sheetTitle>
                    <cshFile>LLL:EXT:listfeusers/csh/locallang_csh_ff.xml</cshFile>
                </TCEforms>
                <type>array</type>
                <el>
                    <mapWidth>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:mapWidth</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config>
                        </TCEforms>
                    </mapWidth>

                    <mapHeight>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:mapHeight</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config>
                        </TCEforms>
                    </mapHeight>


                    <initialMapType>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:mapType</label>
                            <config>
                                <type>radio</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:mapTypeRoadmap</numIndex>
                                        <numIndex index="1">roadmap</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:mapTypeSatellite</numIndex>
                                        <numIndex index="1">satellite</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:mapTypeHybrid</numIndex>
                                        <numIndex index="1">hybrid</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:mapTypeTerrain</numIndex>
                                        <numIndex index="1">terrain</numIndex>
                                    </numIndex>
                                </items>
                                <default>roadmap</default>
                            </config>
                        </TCEforms>
                    </initialMapType>

                    <userGroups>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:userGroup</label>
                            <config>
                                <type>select</type>
                                <foreign_table>fe_groups</foreign_table>
                                <foreign_table_where>ORDER BY fe_groups.title</foreign_table_where>
                                <size>10</size>
                                <maxitems>50</maxitems>
                            </config>
                        </TCEforms>
                    </userGroups>
                    <pid>
                        <TCEforms>
                            <label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>22</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </pid>
                </el>
            </ROOT>
        </default>

        <mapControls>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:listfeusers/pi3/locallang.xml:controlsTitle</sheetTitle>
                    <cshFile>LLL:EXT:listfeusers/csh/locallang_csh_ff.xml</cshFile>
                </TCEforms>
                <type>array</type>
                <el>
                    <mapControlSize>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:conLargeMap</label>
                            <config>
                                <type>radio</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:conLargeMapL</numIndex>
                                        <numIndex index="1">large</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:conLargeMapS</numIndex>
                                        <numIndex index="1">small</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:conLargeMapZ</numIndex>
                                        <numIndex index="1">zoomonly</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:listfeusers/pi3/locallang.xml:conNone</numIndex>
                                        <numIndex index="1">none</numIndex>
                                    </numIndex>
                                </items>
                                <default>3</default>
                            </config>
                        </TCEforms>
                    </mapControlSize>

                    <overviewMap>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:conOverview</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </overviewMap>

                    <scale>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:conScale</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </scale>

                    <mapType>
                        <TCEforms>
                            <label>LLL:EXT:listfeusers/pi3/locallang.xml:conType</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </mapType>
                </el>
            </ROOT>
        </mapControls>
    </sheets>
</T3DataStructure>
